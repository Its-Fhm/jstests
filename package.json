{
  "author": "Faheem",
  "name": "jstests",
  "description": "A test-driven assessment of JavaScript skills",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Its-Fhm/jstests.git"
  },
  "engines": {
    "node": "^4.2.4"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "chai": "^2.3.0",
