    "jquery": "^3.0.0", 
    "mocha": "^2.2.4",
    "sinon": "^1.14.1",
    "underscore": "1.8.x"
  },
  "scripts": {
    "start": "browser-sync start --server --files='app/**/*.js' --index='tests/runner.html'",
